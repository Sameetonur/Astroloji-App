@using AstrolojiApp.Entity.Concrete
@model IEnumerable<Service>


@foreach (var service in Model)
{
    <div class="col-lg-6" data-aos="fade-up" data-aos-delay="100">
        <div class="service-item item-cyan position-relative">


            <div>
                <h3>@service.Name</h3>
                <p>@service.Title</p>
            </div>
        </div>
    </div>

    <!-- End Service Item -->
}
